{
    "name": "kitano/payment-sips-bundle",
    "description": "SIPS payment system driver for kitano/payment-bundle.",
    "keywords": ["payment","sips"],
    "homepage": "http://github.com/Kitano/KitanoPaymentSipsBundle",
    "type": "symfony-bundle",
    "license": "BSD",
    "authors": [
        {
            "name": "Philippe Levan",
            "email": "philippe.levan@kitpages.fr",
            "homepage": "http://www.kitpages.fr"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:lolautruche/KitanoPaymentBundle.git"
        }
    ],
    "require": {
        "kitano/payment-bundle": "*"
    },
    "autoload": {
        "psr-0": { "Kitano\\PaymentSipsBundle": "" }
    },
    "target-dir": "Kitano/PaymentSipsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
